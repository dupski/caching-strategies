{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "no-console": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "object-literal-sort-keys": false,
    "one-line": false,
    "triple-equals": false,
    "variable-name": false
  },
  "rulesDirectory": []
}